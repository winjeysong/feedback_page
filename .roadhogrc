{
  "entry": "src/index.js",
  "theme": "./src/theme.js",
  "cssModulesExclude": [
    "./src/styled/menu.less"
  ],
  "autoprefixer": {
    "browsers": [
      "last 2 version",
      "ie 8"
    ]
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }]
      ]
    }
  }
}
